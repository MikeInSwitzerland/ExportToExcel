<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Mike Gledhill</Authors>
    <Company>www.mikesknowledgebase.com</Company>
    <Description>Export your DataSet, DataTable and List data into a real Excel file, using 1 line of code.</Description>
    <PackageId>Easy.ExportToExcel</PackageId>
    <Version>1.0.2</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DocumentFormat.OpenXml" Version="2.15.0" />
  </ItemGroup>

</Project>
